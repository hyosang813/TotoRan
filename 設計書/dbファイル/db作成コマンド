sqlite3 toto.db

PRAGMA foreign_keys = ON;

BEGIN TRANSACTION;

create table kaisu(open_number text primary key,start_date datetime not null,end_date datetime not null);

create table kumiawase(
kaisu text not null,
id  integer  not null,
get_date datetime,
home_team text not null,
away_team text not null,
home_rate real,
draw_rate real,
away_rate real,
home_rate_b real,
draw_rate_b real,
away_rate_b real,
PRIMARY KEY(kaisu,id),
FOREIGN KEY(kaisu) REFERENCES kaisu(open_number)
);

create table abbname(fullname text not null,name text not null);

